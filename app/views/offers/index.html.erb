<div class="container">

<div class="page-header">
  <h1>Listing Offers</h1>
  <ul class="actions">
        <li><%= link_to "New Offer", new_offer_path, class: "new" %></li>
  </ul>
</div>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Employer</th>
      <th>Country</th>
      <th>City</th>
      <th>Sector</th>
      <th>Posted</th>
      <th>Deadline</th>
      <th>Salary</th>
      <th>Label</th>
      <th>Type</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offers.each do |offer| %>
      <tr>
        <td><%= link_to offer.name, offer %></td>
        <td><%= offer.employer %></td>
        <td><%= offer.country %></td>
        <td><%= offer.city %></td>
        <td><%= offer.sector %></td>
        <td><%= offer.posted %></td>
        <td><%= offer.deadline %></td>
        <td><%= offer.salary %></td>
        <td><%= offer.label %></td>
        <td><%= offer.type %></td>

        <td><%= link_to 'Show', offer %></td>
      </tr>
    <% end %>
  </tbody>

</table>

<br>

